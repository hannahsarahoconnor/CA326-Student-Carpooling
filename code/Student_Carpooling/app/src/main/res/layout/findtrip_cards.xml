                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:id="@+id/Dusername"
                android:text="Username"
                android:textStyle="bold"

                />
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>


    <LinearLayout android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >



        <ImageView
